<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="SecondKeywordMapper">
	<resultMap type="SecondKeyword" id="SecondKeywordResultMap">
		<id 	column="KEYWORD_NO" 	property="keywordNo"/>
		<result column="KEYWORD" 		property="SecondKeyword"/>
	</resultMap>
	<select id="selectSecondKeyword" resultMap="SecondKeywordResultMap">
		SELECT * FROM SECOND_KEYWORD
	</select>
</mapper>