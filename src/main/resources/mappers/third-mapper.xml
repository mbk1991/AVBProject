<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ThirdKeywordMapper">
	<resultMap type="ThirdKeyword" id="ThirdKeywordResultMap">
		<id 	column="KEYWORD_NO" 	property="keywordNo"/>
		<result column="KEYWORD" 		property="ThirdKeyword"/>
	</resultMap>
	<select id="selectThirdKeyword" resultMap="ThirdKeywordResultMap">
		SELECT * FROM THIRD_KEYWORD
	</select>
</mapper>